{
    "extends": ["next", "next/core-web-vitals", "prettier"],
    "rules": {
        "quotes": ["error", "single"],
        "eqeqeq": "error",
        "curly": "error",
        "no-unused-vars": "warn",
        "no-console": "warn",
        "react/prop-types": "off",
        "react/react-in-jsx-scope": "off"
    }
}
